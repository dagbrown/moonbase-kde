mquery SYS_PROXY     "Use system network proxies by default?" n "-DFEATURE_system_proxies=ON" "-DFEATURE_system_proxies=OFF"
mquery USE_JOURNALD  "Send logging output to journald?" n "-DQT_FEATURE_journald=ON" "-DQT_FEATURE_journald=OFF"
mquery USE_CCACHE    "Use ccache?" y "-DQT_USE_CCACHE=ON" "-DQT_USE_CCACHE=OFF"

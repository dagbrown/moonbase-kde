      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)/portingAids
      SOURCE_VFY=sha256:d863872adf437594a592aef6cc589f745ed2d5bd9ada9d4aa0ce64e9a820fc0d

          MODULE=kross
         VERSION=5.112.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)/portingAids
      SOURCE_VFY=sha256:4ac324d161c5ec29931a6c5589ed5bd02d532d0f69d6024629d98e46fa3a08ad

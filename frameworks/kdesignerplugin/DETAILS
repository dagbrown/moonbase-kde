          MODULE=kdesignerplugin
         VERSION=5.88.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)/portingAids/
      SOURCE_VFY=sha256:d90c6245209d52e891755fcf8a35ce40d9de3b9c315c3f4bef8b556270b0154b

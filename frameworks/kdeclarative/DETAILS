          MODULE=kdeclarative
         VERSION=5.93.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:08a945c793aa8d89d87c42d29c9c11e7c28c47916ae728369ab478892d325067

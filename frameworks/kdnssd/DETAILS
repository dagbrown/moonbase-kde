          MODULE=kdnssd
         VERSION=5.103.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:267aab6d1bdfb2be68e65c6c4efd5ba638dea6f913bc2016f187c5b8f13645ed

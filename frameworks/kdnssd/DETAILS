          MODULE=kdnssd
         VERSION=5.108.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:c83292582bac3f499f2429a1c68156d23068434df2b49b9c74ff092a259fb45f

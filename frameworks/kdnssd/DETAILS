          MODULE=kdnssd
         VERSION=6.10.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:fd8f08501fc40b9a29fc370f7f7871083f2489922e1bd50bc5b0fd78017daa27

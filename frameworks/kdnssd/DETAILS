          MODULE=kdnssd
         VERSION=5.112.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:5adc0440d4ca8adea12094304824355f3407654b02b6885a12560447486da131

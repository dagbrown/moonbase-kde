          MODULE=cervisia
         VERSION=23.08.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:9d00461a980ce27398a7703bf996fdafe4f6b65355209041a9e5de3be6c2e856

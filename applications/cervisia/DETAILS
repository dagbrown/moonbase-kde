          MODULE=cervisia
         VERSION=24.08.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:d284c0778f2c59fa140b021a1dac82cf17b43d7725fb5be05b5a15e1ee3bd8cf

          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:b86cc3df08f1651d30a278a372f94e326899fcb1416e6de979e8148501468ebd

          MODULE=cervisia
         VERSION=21.08.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:ae5656a062cd1df20a6e4af9e20ef4aee4629b611e6832e8eff2c7e9b08e9074

          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:08f900f9ef770b2bc9208781e5dcdb7e0c43f0c7a47a1515b1888f24756542f7

          MODULE=libktorrent
         VERSION=22.12.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:4deee1265c646d062a91170882ec939d25049546d7488a65d49a9b7d4c467392

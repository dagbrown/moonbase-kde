          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:28fdc47f348fee51c7716ac832563b305acbec52c9d29be16eb37c99c00e23bf

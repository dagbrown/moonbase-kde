          MODULE=libktorrent
         VERSION=22.04.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:381d6c05bd536492a43343e48785c8170549080f3d938db736fb285ed6610a25
        WEB_SITE=https://www.kde.org/applications/internet/ktorrent/

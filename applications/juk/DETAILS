          MODULE=juk
         VERSION=23.08.4
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:595d2138756ab360929d3f9fc9faca0c2f65090511e86847105e8720512029ab

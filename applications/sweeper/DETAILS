          MODULE=sweeper
         VERSION=22.12.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:92b10562ef5eff2744f55645b888cbe0b7db7c97f06d26eb125ba36953abc62a

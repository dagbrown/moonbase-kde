          MODULE=sweeper
         VERSION=22.12.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:80b5946b9608fefc9978715d2df521f691624f9eb86430365a9ce7d87e172933

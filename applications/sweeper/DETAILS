          MODULE=sweeper
         VERSION=24.12.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:fbcbaaace8889857e9d76c01ad26dcbd649b6884ec26dbaedcd723775eff37e9

          MODULE=sweeper
         VERSION=21.08.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:b27c04fab114f738d05926c02e28cae6d0507820dd67bceff0395523ba6b4869

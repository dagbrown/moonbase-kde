          MODULE=libksane
         VERSION=23.08.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:eabdcac6f2231fba63e13e59e2be7e5b7b0e5973c42e1c36220472a593bb8e36
